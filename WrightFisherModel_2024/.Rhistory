model_vals <- rep(0, length(unlist(observed))-4)
data_missing <- FALSE
for (i in 1:(length(unlist(observed))-4)){
state_name <- paste("sum_clust", i, sep = "")
model_vals[i] <- state[state_name, , drop = TRUE]
if (is.na(observed[[as.character(i)]])) {
